{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@/libs/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@/libs/database": "<rootDir>/../libs/database/src",
    "@/libs/config/(.*)": "<rootDir>/../libs/config/src/$1",
    "@/libs/config": "<rootDir>/../libs/config/src",
    "@/libs/passport/(.*)": "<rootDir>/../libs/passport/src/$1",
    "@/libs/passport": "<rootDir>/../libs/passport/src",
    "@/libs/cache/(.*)": "<rootDir>/../libs/cache/src/$1",
    "@/libs/cache": "<rootDir>/../libs/cache/src"
  }
}
